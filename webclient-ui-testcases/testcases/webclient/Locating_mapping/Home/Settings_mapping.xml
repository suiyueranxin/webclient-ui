    	<ByValue>userSettingSaveButton</ByValue>
    	<ByType>id</ByType>
    </Item>
    <!-- Notifications -->
    <Item>
  		<Name>Notifications</Name>
    	<ByValue>//div[@class='sapMSLITitle' and text()='Notifications']</ByValue>
    	<ByType>xpath</ByType>
    </Item>
    <Item>
  		<Name>Show Activity Reminders</Name>
    	<ByValue>doNotDisturbSwitch</ByValue>
    	<ByType>id</ByType>
    </Item>
    <Item>
  		<Name>Show Preview in Home Page</Name>
    	<ByValue>enablePreviewSwitch-switch</ByValue>
